{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": [
        true,
        "never-prefix"
      ],
        "variable-name": [true,
            "check-format",
            "allow-leading-underscore"
        ],
        "max-line-length": [true, 255],
        "member-ordering": [
          false,
          {
            "order": [
              "public-static-field"
            ]
          }
        ],
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}